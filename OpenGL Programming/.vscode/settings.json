{
    "run.timelimit": 300000,
    "run.compiler": "\"${workspaceFolder}\\mingw32\\bin\\g++.exe\"",
    "run.compilerArgs": "-lfreeglut -lopengl32 -lglu32 -lwinmm -lgdi32 -Wl,--subsystem,windows",
    "run.compilerDirectory": "${workspaceFolder}\\mingw32\\bin\\",
    "C_Cpp.formatting": "vcFormat",
    "breadcrumbs.enabled": false,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "mingw*": true,
        "Program.exe": true
    },
    "run.inputFile": "${workspaceFolder}\\Input.txt",
    "run.outputFile": "${workspaceFolder}\\Output.txt",
    "files.associations": {
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp"
    }
}